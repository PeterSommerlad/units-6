INPUT = @CMAKE_SOURCE_DIR@
EXCLUDE_SYMBOLS = detail

PROJECT_NAME = @PROJECT_NAME@
PROJECT_NUMBER = @GIT_REPO_VERSION@

JAVADOC_AUTOBRIEF = YES

HTML_OUTPUT= @CMAKE_SOURCE_DIR@/docs

GENERATE_LATEX = NO
CLASS_DIAGRAMS = NO
CLASS_GRAPH = NO
COLLABORATION_GRAPH = NO
UML_LOOK = NO
GRAPHICAL_HIERARCHY = NO
DIRECTORY_GRAPH = NO
GENERATE_LEGEND = NO

FILE_PATTERNS = *.h

HTML_HEADER = @CMAKE_SOURCE_DIR@/doxygen/header.html
HTML_FOOTER = @CMAKE_SOURCE_DIR@/doxygen/footer.html
HTML_STYLESHEET = @CMAKE_SOURCE_DIR@/doxygen/customdoxygen.css
HTML_EXTRA_FILES = @CMAKE_SOURCE_DIR@/doxygen/doxy-boot.js
